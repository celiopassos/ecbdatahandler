# TABLES

[medicao_m3]
table = medicao_m3
type = m3
not_null = cacamba_no, placa, data
rename = m3:qtd, m3xpuxkm:preco, cacamba_no:ca, total_kms_parte_diaria:parte_diaria, total_km_estaca:estaca
price = material_pack_1:0.48
null_price_map = 0.56:0.48, 0.40:0.34

[medicao_ton]
table = medicao_ton
type = ton
not_null = cacamba_no, placa, data
rename = ton:qtd, tonxpuxkm:preco, cacamba_no:ca, total_kms_parte_diaria:parte_diaria, total_km_estaca:estaca
price = material_pack_2:0.34
null_price_map = 0.56:0.48, 0.40:0.34

[combustivel_]
table = combustivel
not_null = placa
rename = tipo_de_combustivel:combustivel
price = combustivel_pack_1:2.7924


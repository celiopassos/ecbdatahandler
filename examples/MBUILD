[medicao]
names = medicao_m3, medicao_ton
columns = Data:7, CA:5, Placa:6, Motorista:25, Qtd.:4, Unid.:5, Preço:6, MATERIAL:25, KM Inicial:9, KM Final:9, Retorno:8, Parte Diária:10, Estaca:6, Acerto:6, Valorização:20

[combustivel]
